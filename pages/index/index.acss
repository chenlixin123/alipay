}
.add_img{
  width:40rpx;
  height:40rpx;
  position: absolute;
  right: 0;
  top:0;
}
.choice{
  width:40rpx;
  height:40rpx;
}
.plate_number_box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 15rpx;
  font-size: 35rpx;
}
.indexStyle {
  width: 100%;
  text-align: center;
  display: flex;

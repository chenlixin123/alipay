.box{
  width: 100%;
  height: 100vh;
}
.Floating_layer{
  border-top: 1px solid #efefef;
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content:space-between;
  align-items: center;
  font-weight: bold;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
}
.Floating_layer_left{
  margin-left: 60px;
}
.Floating_layer_right{
  margin-right: 60px;
}
.Pop_up{
  width: 70%;
  height: 30vh;
  background: #fff;
  margin-top: 30vh;
  border-radius: 10px;
}
.module_occupy_number{
  color: #ff8234;
  font-weight: bold;
}
.deleteBtn {
  color: #f93a4a;
  font-weight: bolder;
}
.cancelBtn {
  color: #ccc;
}
.buttonBold, .modalButtonBold .am-modal-footer {
  font-weight: bold;
  color: #ff8234;
}
.buttoncancel{
  color: #999;
}
.module_explain_box{
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .5)
}
.module_explain_title{
  font-weight: bold;
  text-align: center;
  margin-bottom: 20rpx;
}
.module_explain_text{
  color: #ff8234;
}
.br{
  width: 100%;
  border-top: 1px solid rgb(241, 241, 241);
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.module_explain_text_title{
  display: flex;
  justify-content: space-around;
}
.module_explain_text_text{
  display: flex;
  color:#ff8234;
  margin-bottom: 15rpx;
}
.module_explain_text_title1{
  width:33.3%;
